{
  "name": "full-siora-mvp",
  "private": true,
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
  "dev": "turbo run dev",
  "build": "prisma generate --schema=./prisma/schema.prisma && turbo run build --filter=apps/web",
  "dev:web": "npm run dev -w apps/web",
  "build:web": "prisma generate --schema=./prisma/schema.prisma && npm run build -w apps/web",
  "lint": "turbo run lint",
  "start": "turbo run start",
  "generate:campaign": "node tools/campaign_flow.mjs",
  "postinstall": "prisma generate",
  "prisma:generate": "prisma generate --schema=./prisma/schema.prisma"
},
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "^24.0.10",
    "autoprefixer": "^10.4.14",
    "postcss-import": "^16.1.1",
    "prisma": "^6.9.0",
    "tailwindcss": "^4.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "turbo": "latest"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3",
    "next": "^15.3.4",
    "pdfkit": "^0.15.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "prisma": {
     "schema": "./prisma/schema.prisma",
  "seed": "tsx prisma/seed.ts"
}
}
